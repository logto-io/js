		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/cookie": "^0.6.0",
		"eslint": "^9.0.0",
		"lint-staged": "^15.0.0",
		"prettier": "^3.0.0",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tslib": "^2.4.1",
		"typescript": "^5.3.3",
		"vite": "^5.2.9"
	},
	"eslintConfig": {
		"extends": "@silverhand"
	},
	"prettier": "@silverhand/eslint-config/.prettierrc"
