{
	"name": "@logto/sveltekit-sample",
	"version": "0.2.0",
	"private": true,
	"type": "module",
	"scripts": {
		"precommit": "lint-staged",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
