{
  "name": "@logto/browser-sample",
  "version": "1.0.0-beta.3",
  "license": "MIT",
  "private": true,
  "scripts": {
    "precommit": "lint-staged",
    "start": "parcel public/index.html -p 3000",
    "build": "rm -rf dist && parcel build public/index.html --no-autoinstall",
    "lint": "eslint --ext .js src",
    "stylelint": "stylelint \"src/**/*.scss\""
  },
  "dependencies": {
    "@logto/browser": "^1.0.0-beta.3"
  },
  "devDependencies": {
    "@parcel/core": "^2.6.2",
    "@parcel/transformer-sass": "^2.6.2",
    "@silverhand/eslint-config": "^0.17.0",
    "@silverhand/eslint-config-react": "^1.0.0",
    "buffer": "^6.0.3",
    "eslint": "^8.9.0",
    "lint-staged": "^13.0.0",
    "parcel": "^2.6.2",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "process": "^0.11.10",
    "stylelint": "^14.8.2",
    "typescript": "^4.6.2"
  },
  "eslintConfig": {
    "extends": "@silverhand",
    "globals": {
      "window": true,
      "document": true
    }
  },
  "stylelint": {
    "extends": "@silverhand/eslint-config-react/.stylelintrc"
  },
  "prettier": "@silverhand/eslint-config/.prettierrc"
}
