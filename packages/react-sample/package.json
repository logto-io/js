{
  "name": "@logto/react-sample",
  "version": "1.0.0-rc.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "precommit": "lint-staged",
    "start": "parcel src/index.html -p 3000",
    "check": "tsc --noEmit",
    "build": "pnpm check && rm -rf dist && parcel build src/index.html --no-autoinstall",
    "lint": "eslint --ext .ts --ext .tsx src",
    "stylelint": "stylelint \"src/**/*.scss\""
  },
  "dependencies": {
    "@logto/react": "^1.0.0-rc.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2"
  },
  "devDependencies": {
    "@parcel/core": "^2.8.3",
    "@parcel/transformer-sass": "^2.8.3",
    "@silverhand/eslint-config": "^1.0.0",
    "@silverhand/eslint-config-react": "^1.0.0",
    "@silverhand/ts-config": "^1.0.0",
    "@silverhand/ts-config-react": "^1.0.0",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.3",
    "buffer": "^6.0.3",
    "eslint": "^8.23.0",
    "lint-staged": "^13.0.0",
    "parcel": "^2.8.3",
    "postcss": "^8.4.6",
    "postcss-modules": "^6.0.0",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "stylelint": "^15.0.0",
    "typescript": "4.9.5"
  },
  "eslintConfig": {
    "extends": "@silverhand/react"
  },
  "stylelint": {
    "extends": "@silverhand/eslint-config-react/.stylelintrc"
  },
  "prettier": "@silverhand/eslint-config/.prettierrc"
}
